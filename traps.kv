<SpiderWeb>:
    canvas.before:
        Rectangle:
            size: self.web_width, self.web_height
            pos: self.pos[0], self.web_position
            texture: self.web_texture
<Bit>:
    canvas.before:
        Rectangle:
            size: self.bit_width, self.bit_height
            pos: self.pos[0], self.bit_position
            texture: self.bit_texture
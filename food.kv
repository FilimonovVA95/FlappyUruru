<BloodBag>:
    canvas.before:
        Rectangle:
            size: self.blood_width, self.blood_height
            pos: self.pos[0], self.blood_position
            texture: self.blood_texture
<EnergyDrink>:
    canvas.before:
        Rectangle:
            size: self.drink_width, self.drink_height
            pos: self.pos[0], self.drink_position
            texture: self.drink_texture